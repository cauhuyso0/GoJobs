<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item
  android:left="-300dp"
  android:right="-300dp">
  <rotate
    android:fromDegrees="90"
    android:toDegrees="90">
    <shape android:shape="line">
    <stroke
      android:width="1px"
      android:color="#495057"
      android:dashGap="10px"
      android:dashWidth="10px" />
    </shape>

  </rotate>
</item>
</layer-list>